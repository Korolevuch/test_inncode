
<h1>Listing Feeds</h1>
 
<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>Is active?</th>
      <th>Created</th>
      <th>Updated</th>
      <th colspan="3"></th>
    </tr>
  </thead>
 
  <tbody>
    <% @feeds.each do |feed| %>
      <%= render partial: "feed", locals: { feed: feed } %>
    <% end %>
  </tbody>
</table>
 